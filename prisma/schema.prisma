// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model users {
  id              Int         @id @default(autoincrement())
  email           String      @unique
  fullName        String
  bithday         String?
  password        String 
  role            Role        @default(USER)
}


enum Role {
  USER
  ADMIN
}